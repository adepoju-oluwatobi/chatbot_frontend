{
    "builds": [
      {
        "src": "dist/my-widget.umd.js",  
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/my-widget.umd.js",  
        "destination": "/dist/my-widget.umd.js"  
      }
    ],
    "headers": [
      {
        "source": "/my-widget.umd.js", 
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }
  