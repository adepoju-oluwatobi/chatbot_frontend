{
    "builds": [
      {
        "src": "dist/my-widget.umd.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/my-widget.umd.js", 
        "dest": "/dist/my-widget.umd.js" 
      }
    ],
    "headers": [
      {
        "source": "/my-widget.umd.js",
        "headers": [
          {
            "key": "Content-Type",  
            "value": "application/javascript"
          }
        ]
      }
    ]
  }
  